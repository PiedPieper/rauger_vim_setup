execute pathogen#infect()

syntax enable
set autoindent
set backspace=2
set hlsearch

set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab



set number
set virtualedit=all
set wildmode=longest,list,full
set wildmenu

filetype plugin on

let mapleader="g"

nmap <S-Enter> O<Esc>
nmap <CR> o<Esc>


colorscheme atom-dark-256
